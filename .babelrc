{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": 3,
        "targets": [
          "last 2 versions",
          "not dead",
          "not < 2%"
        ],
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
